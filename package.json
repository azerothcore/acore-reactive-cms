{
  "name": "acore-reactive-cms-root",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter acore-reactive-cms-website...",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "turbo": "2.3.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18",
        "react-dom": "18",
        "@types/react": "18"
      }
    }
  }
}
